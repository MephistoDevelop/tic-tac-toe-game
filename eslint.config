{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "document": true,
    "foo": true,
    "window": true
  },
  "parserOptions": { "ecmaVersion": 2018 },
  "extends": "airbnb-base",
    rules: {
    'no-restricted-globals': ["error", "event", "fdescribe"],
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
  },
  "type": "array",
  "items": {
    "type": "string",
    "enum": [
      "allow-fast-null-checks",
      "allow-new",
      "allow-tagged-template"
    ]
  },
  "minLength": 0,
  "maxLength": 3
}
